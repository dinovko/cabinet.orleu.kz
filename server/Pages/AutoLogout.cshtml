@page
@model server.cabinet.orleu.kz.Pages.AutoLogoutModel
@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Logging out...</title>
    <meta name="RequestVerificationToken" content="@Model.AntiForgeryToken" />
</head>
<body>
    <form id="logoutForm" method="post" action="/Logout">
        <input name="__RequestVerificationToken" type="hidden" value="@Model.AntiForgeryToken" />
    </form>
    <script>
        document.getElementById('logoutForm').submit();
    </script>
</body>
</html>